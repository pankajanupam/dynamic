<fieldset class="fieldset">
    <legend>Config</legend>
    <div class="grid-x grid-margin-x">
        {{#each form.config}}
            {{#ifequal type 'select'}}
                {{> selectbox}}
            {{else ifequal type 'checkbox'}}
                {{> checkbox}}
            {{/ifequal}}
        {{/each}}
    </div>
</fieldset>